{
  "name": "codementordevtest",
  "version": "1.0.0",
  "scripts": {
    "dev": "next",
    "build": "next build",
    "start": "next start -p $PORT",
    "test": "jest --detectOpenHandles"
  },
  "dependencies": {
    "algoliasearch": "^4.8.6",
    "lodash": "^4.17.21",
    "next": "latest",
    "prettier": "^2.2.1",
    "react": "^16.8.0",
    "react-dom": "^16.8.0",
    "react-is": "^16.8.0"
  },
  "devDependencies": {
    "@testing-library/jest-dom": "^5.11.10",
    "@testing-library/react": "^11.2.5",
    "@testing-library/user-event": "^13.0.16",
    "@types/jest": "^26.0.20",
    "@types/node": "^14.14.34",
    "@types/react": "^17.0.3",
    "jest": "^26.6.3",
    "ts-jest": "^26.5.3",
    "typescript": "^4.2.3"
  },
  "license": "MIT",
  "jest": {
    "testRegex": "(/__tests__/.*|(\\.|/)(test|spec))\\.(tsx?)$",
    "setupFilesAfterEnv": [
      "<rootDir>/jest.preset.js"
    ],
    "coveragePathIgnorePatterns": [
      "/node_modules/",
      "./jest.preset.js"
    ],
    "moduleFileExtensions": [
      "tsx",
      "ts",
      "js",
      "json",
      "node"
    ],
    "collectCoverage": false,
    "globals": {
      "ts-jest": {
        "diagnostics": {
          "warnOnly": true
        }
      }
    }
  }
}